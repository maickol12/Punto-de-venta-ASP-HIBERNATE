<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="esqueletos.caja">
		<id name="idcaja">
			<column name="idcaja"></column>
			<generator class="identity"></generator>
		</id>
		<property name="codigo_caja"></property>
		<property name="f_alta"></property>
		<property name="f_baja"></property>
		<property name="f_update"></property>
		<property name="is_open"></property>
		<property name="is_active"></property>
	</class>
</hibernate-mapping>