<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="esqueletos.cliente" table="cliente">
		<id name="idcliente">
			<column name="idcliente"></column>
			<generator class="identity"></generator>
		</id>
		<property name="razon_social" type="java.lang.String">
			<column name="razon_social"></column>
		</property>
		<property name="rfc" type="java.lang.String">
			<column name="rfc"></column>
		</property>
		<property name="calle" type="java.lang.String">
			<column name="calle"></column>
		</property>
		<property name="numero" type="int">
			<column name="numero"></column>
		</property>
		<property name="ciudad" type="java.lang.String">
			<column name="ciudad"></column>
		</property>
		<property name="municipio" type="java.lang.String">
			<column name="municipio"></column>
		</property>
		<property name="estado" type="java.lang.String">
			<column name="estado"></column>
		</property>
		<property name="pais" type="java.lang.String">
			<column name="pais"></column>
		</property>
		<property name="cp" type="java.lang.String">
			<column name="cp"></column>
		</property>
		<property name="email" type="java.lang.String">
			<column name="email"></column>
		</property>
		<property name="telefono" type="java.lang.String">
			<column name="telefono"></column>
		</property>
		<property name="f_alta" type="java.util.Date">
			<column name="f_alta"></column>
		</property>
		<property name="f_baja" type="java.util.Date">
			<column name="f_baja"></column>
		</property>
		<property name="is_active" type="int">
			<column name="is_active"></column>
		</property>
	</class>
</hibernate-mapping>